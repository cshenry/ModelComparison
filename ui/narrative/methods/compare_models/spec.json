            "text_options" : {
                "valid_ws_types" : [ "KBaseFBA.ModelComparison" ],
                "is_output_name":true
            }
        },
        {
            "id": "model_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFBA.FBAModel" ]
            }
        },
        {
            "id": "protcomp_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "GenomeComparison.ProteomeComparison" ]
            }
        },
        {
            "id": "pangenome_ref",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomes.Pangenome" ]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "ModelComparison",
            "method": "compare_models",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "mc_name",
                    "target_property": "mc_name"
                },
                {
                    "input_parameter": "model_ref",
                    "target_property": "model_refs",
